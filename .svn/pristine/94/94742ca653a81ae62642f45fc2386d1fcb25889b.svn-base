	<script type="text/javascript" src="../../script/api.js"></script>
	<script type="text/javascript" src="../../script/aui-popup.js" ></script>
	<script type="text/javascript" src="../../script/my_ajax.js"></script>
	<script type="text/javascript" src="../../script/common.js"></script>
	<script>
		apiready = function() {
			//沉浸式
			var headbar = $api.byId('headbar');
			$api.fixIos7Bar(headbar);
			var doclistId = api.pageParam.doclistId;
			if(!doclistId || doclistId == ""){
				api.toast({
					msg : '请重新选择查看信息',duration : 2000,location : 'center'
				});				
			}else{
				// 根据文档ID获取详情内容
				findDocCont(doclistId);
			}
		};
		function findDocCont(docId){
			var ajaxObject = new Object();
			ajaxObject.url = "/appPortalDoclistAjax!queryPortalDoclistViewVo.htm";
			ajaxObject.params = new Object();
			ajaxObject.params.doclistId = docId;
			ajaxObject.onSuccessFunction = function(data) {
				getWorkBookHtmlByList(data.portalDoclistViewVo, "docCont");
			}
			baseAjax(ajaxObject);			
		}
	</script>
