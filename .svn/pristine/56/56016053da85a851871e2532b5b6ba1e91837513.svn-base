	<script>
 	apiready = function() {
		var headbar = $api.byId('headbar');
		$api.fixIos7Bar(headbar);
		queryComProtocol(); 		// 协议
	};
	
	function queryComProtocol(){
		var ajaxObject = new Object();
		ajaxObject.url = "/appPortalDoclistAjax!queryPortalDoclistViewVo.htm";
		ajaxObject.params = new Object();
		ajaxObject.params.doclistId = "10310";	// 查询指定文档信息
		ajaxObject.onSuccessFunction = function(data) {
			var data = data.portalDoclistViewVo;
			getWorkBookHtmlByList(data,"comProtocol");
		}
		
		baseAjax(ajaxObject);	
		
	}
	
	</script>
