        <li class="aui-list-item">
            <div class="aui-list-item-inner">
                <div class="aui-list-item-title">应付金额</div>
                <div class="aui-list-item-right">{dataParam}元</div>
            </div>
        </li>
        <li class="aui-list-item">
            <div class="aui-list-item-inner">
                <div class="aui-list-item-title">优惠金额</div>
                <div class="aui-list-item-right">{dataCallurl}元</div>
            </div>
        </li>
        <li class="aui-list-item">
            <div class="aui-list-item-inner">
                <div class="aui-list-item-title">实付金额</div>
                <div class="aui-list-item-right">{pricesetPrice}元</div>
            </div>
        </li>
         <li class="aui-list-item">
            <div class="aui-list-item-inner">
                <div class="aui-list-item-title">订单编号</div>
                <div class="aui-list-item-right">{mgpriceBatch}</div>
            </div>
        </li>
        <li class="aui-list-item">
            <div class="aui-list-item-inner">
                <div class="aui-list-item-title">支付方式</div>
                <div class="aui-list-item-right">{payTypeStr}</div>
            </div>
        </li>
        <li class="aui-list-item">
            <div class="aui-list-item-inner">
                <div class="aui-list-item-title">订单时间</div>
                <div class="aui-list-item-right">{mgpriceDateStr}</div>
            </div>
        </li>
    </ul>
</div>
    
    
</body>
<script type="text/javascript" src="../../../script/api.js"></script>
<script type="text/javascript" src="../../../script/aui-tab.js"></script>
<script type="text/javascript" src="../../../script/jquery.min.js"></script>
<script type="text/javascript" src="../../../script/common.js"></script>
<script type="text/javascript" src="../../../script/my_ajax.js"></script>

<script>
	apiready = function(){
	 	var batch = api.pageParam.batch;
	 	queryMgpriceCon(batch);
	}
      
    //  查询订单详情 
    function queryMgpriceCon(mgpriceBatch){   	
    	var ajaxObject = new Object();
    	ajaxObject.url = "/appGasStoreOrderAjax!queryGasStoreOrderViewVo.htm";
		ajaxObject.params = new Object();
		ajaxObject.params.mgpriceBatch =  mgpriceBatch;
		ajaxObject.onSuccessFunction = function(ret) {
			var data = ret.resourceMgpriceViewVo;
			if(data.mgpriceBusinessDeviation == 0){//金额支付
				data.mgpriceBusinessDeviation = Number(data.dataParam) / Number(data.dataCalltype);
			}else{//数量支付
				data.pricesetPrice = data.dataParam;
			}
			if(data.payType == 3 || data.payType == 4){//团购券预购券支付
				data.dataCallurl = 0;
			}
			data.mgpriceBusinessDeviation = toFixed(data.mgpriceBusinessDeviation);
			data.dataCallurl = toFixed(data.dataCallurl);
			data.dataParam = toFixed(data.dataParam);
			getWorkBookHtmlByList(data, "mgpriceCon");	
			//列表图片缓存
			$(".imgClass").each(function(index){
				imageCache(this);
			});		
		}
		baseAjax(ajaxObject);    	
    }    
</script>
</html>