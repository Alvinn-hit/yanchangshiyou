<body id="mgpriceCon" style="display:none ">
    <div class="list-box-flex">
    <div class="list-box1-td1 td-img">
      <img src="" datasrc="{baseURL}/{dataStr12}" class="imgClass"></img>
    </div>
    <div class="list-box1-td2">
      <h1 class="list-title">{memberBname}</h1>
       <div class="list-line">
        <span class="list-dizhi">
           {mgpriceDescription}&nbsp;|&nbsp;{glistBillbatchOld}
        </span>
      </div>
      <div class="list-line">
        <span class="list-dizhi">
                           数量：{mgpriceBusinessDeviation}升
        </span>
      </div>
      <div class="list-line">
        <span class="list-dizhi">
                           车牌号：{mgpriceContactname} <i class="aui-pull-right" style="font-style: normal;">{mgpriceDateStr}</i>
        </span>
      </div>
    </div>   
  </div>   
  
  <div class="aui-content aui-margin-t-15">
    
